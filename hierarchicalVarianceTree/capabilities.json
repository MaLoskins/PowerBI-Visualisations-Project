{
    "dataRoles": [
        {
            "displayName": "Category",
            "name": "category",
            "kind": "Grouping",
            "displayNameKey": "Role_Category"
        },
        {
            "displayName": "Actual",
            "name": "actual",
            "kind": "Measure",
            "displayNameKey": "Role_Actual",
            "requiredTypes": [{ "numeric": true }]
        },
        {
            "displayName": "Budget",
            "name": "budget",
            "kind": "Measure",
            "displayNameKey": "Role_Budget",
            "requiredTypes": [{ "numeric": true }]
        },
        {
            "displayName": "Tooltip Fields",
            "name": "tooltipFields",
            "kind": "GroupingOrMeasure",
            "displayNameKey": "Role_Tooltip"
        }
    ],
    "objects": {
        "layoutSettings": {
            "properties": {
                "orientation": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Top Down", "value": "topDown" },
                            { "displayName": "Left to Right", "value": "leftRight" }
                        ]
                    }
                },
                "nodeWidth": {
                    "type": { "numeric": true }
                },
                "nodeHeight": {
                    "type": { "numeric": true }
                },
                "levelSpacing": {
                    "type": { "numeric": true }
                },
                "siblingSpacing": {
                    "type": { "numeric": true }
                },
                "nodeCornerRadius": {
                    "type": { "numeric": true }
                }
            }
        },
        "nodeSettings": {
            "properties": {
                "showVarianceBar": {
                    "type": { "bool": true }
                },
                "barHeight": {
                    "type": { "numeric": true }
                },
                "showPercentage": {
                    "type": { "bool": true }
                },
                "showAbsoluteValue": {
                    "type": { "bool": true }
                },
                "nodeFontSize": {
                    "type": { "numeric": true }
                },
                "nodeFontColor": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "valueFontSize": {
                    "type": { "numeric": true }
                },
                "nodeBackground": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "nodeBorderColor": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "nodeBorderWidth": {
                    "type": { "numeric": true }
                }
            }
        },
        "colorSettings": {
            "properties": {
                "favourableColor": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "unfavourableColor": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "neutralColor": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "connectorColor": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                },
                "connectorWidth": {
                    "type": { "numeric": true }
                },
                "selectedNodeBorder": {
                    "type": {
                        "fill": { "solid": { "color": true } }
                    }
                }
            }
        },
        "interactionSettings": {
            "properties": {
                "startExpanded": {
                    "type": {
                        "enumeration": [
                            { "displayName": "All", "value": "all" },
                            { "displayName": "Root Only", "value": "rootOnly" },
                            { "displayName": "None", "value": "none" }
                        ]
                    }
                },
                "animationDuration": {
                    "type": { "numeric": true }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        { "for": { "in": "category" } },
                        { "for": { "in": "actual" } },
                        { "for": { "in": "budget" } },
                        { "for": { "in": "tooltipFields" } }
                    ]
                },
                "dataReductionAlgorithm": {
                    "top": { "count": 10000 }
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": { "default": true, "canvas": true },
        "roles": ["tooltipFields"]
    },
    "privileges": []
}
