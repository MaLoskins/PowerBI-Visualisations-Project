    "objects": {
        "gaugeSettings": {
            "properties": {
                "startAngle": {
                    "type": { "numeric": true }
                },
                "endAngle": {
                    "type": { "numeric": true }
                },
                "arcThickness": {
                    "type": { "numeric": true }
                },
                "arcCornerRadius": {
                    "type": { "numeric": true }
                },
                "backgroundArcColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "backgroundArcOpacity": {
                    "type": { "numeric": true }
                }
            }
        },
        "needleSettings": {
            "properties": {
                "showNeedle": {
                    "type": { "bool": true }
                },
                "needleColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "needleLength": {
                    "type": { "numeric": true }
                },
                "needleWidth": {
                    "type": { "numeric": true }
                },
                "pivotRadius": {
                    "type": { "numeric": true }
                },
                "pivotColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "animationDuration": {
                    "type": { "numeric": true }
                }
            }
