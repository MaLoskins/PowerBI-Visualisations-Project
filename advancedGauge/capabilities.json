                "valueFontColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "valueFormat": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Auto", "value": "auto" },
                            { "displayName": "Number", "value": "number" },
                            { "displayName": "Percent", "value": "percent" },
                            { "displayName": "Currency", "value": "currency" }
                        ]
                    }
                },
                "showMinMaxLabels": {
                    "type": { "bool": true }
                },
                "minMaxFontSize": {
                    "type": { "numeric": true }
                },
                "minMaxFontColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "showTitle": {
                    "type": { "bool": true }
                },
                "titleText": {
                    "type": { "text": true }
                },
                "titleFontSize": {
                    "type": { "numeric": true }
                },
                "titleFontColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                }
            }
