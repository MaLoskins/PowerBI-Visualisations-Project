                "tickWidth": {
                    "type": { "numeric": true }
                },
                "tickColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "showTickLabels": {
                    "type": { "bool": true }
                },
                "tickLabelFontSize": {
                    "type": { "numeric": true }
                },
                "tickLabelFontColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        { "for": { "in": "value" } },
                        { "for": { "in": "target" } },
                        { "for": { "in": "minValue" } },
                        { "for": { "in": "maxValue" } },
                        { "for": { "in": "range1Max" } },
                        { "for": { "in": "range2Max" } },
                        { "for": { "in": "range3Max" } },
                        { "for": { "in": "tooltipFields" } }
                    ],
                    "dataReductionAlgorithm": {
                        "top": { "count": 10000 }
                    }
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": { "default": true, "canvas": true },
        "roles": ["tooltipFields"]
    },
    "privileges": []
}
