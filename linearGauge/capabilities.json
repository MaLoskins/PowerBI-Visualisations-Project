            "displayNameKey": "Role_TooltipFields"
        }
    ],
    "objects": {
        "layoutSettings": {
            "properties": {
                "orientation": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Horizontal", "value": "horizontal" },
                            { "displayName": "Vertical", "value": "vertical" }
                        ]
                    }
                },
                "gaugeHeight": {
                    "type": { "numeric": true }
                },
                "gaugeSpacing": {
                    "type": { "numeric": true }
                },
                "gaugeCornerRadius": {
                    "type": { "numeric": true }
                },
                "categoryWidth": {
                    "type": { "numeric": true }
                },
                "categoryFontSize": {
                    "type": { "numeric": true }
                },
                "categoryFontColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "showCategoryLabels": {
                    "type": { "bool": true }
                }
            }
        },
        "barSettings": {
            "properties": {
                "barColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "barOpacity": {
                    "type": { "numeric": true }
                },
                "trackColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "trackBorderColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "trackBorderWidth": {
                    "type": { "numeric": true }
                }
            }
        },
        "rangeSettings": {
            "properties": {
                "showRanges": {
                    "type": { "bool": true }
                },
                "range1Color": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "range2Color": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "rangeBeyondColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                }
            }
        },
        "targetSettings": {
            "properties": {
                "showTarget": {
                    "type": { "bool": true }
                },
                "targetColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "targetWidth": {
                    "type": { "numeric": true }
                },
                "targetHeight": {
                    "type": { "numeric": true }
                },
                "target2Color": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "target2Width": {
                    "type": { "numeric": true }
                },
                "target2Style": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Solid", "value": "solid" },
                            { "displayName": "Dashed", "value": "dashed" }
                        ]
                    }
                }
            }
        },
        "labelSettings": {
            "properties": {
                "showValueLabel": {
                    "type": { "bool": true }
                },
                "valueLabelPosition": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Inside", "value": "inside" },
                            { "displayName": "Right", "value": "right" },
                            { "displayName": "Left", "value": "left" },
                            { "displayName": "Above", "value": "above" }
                        ]
                    }
                },
                "valueFontSize": {
                    "type": { "numeric": true }
                },
                "valueFontColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "valueFormat": {
                    "type": {
                        "enumeration": [
                            { "displayName": "Auto", "value": "auto" },
                            { "displayName": "Number", "value": "number" },
                            { "displayName": "Percent", "value": "percent" },
                            { "displayName": "Currency", "value": "currency" }
                        ]
                    }
                },
                "showTargetLabel": {
                    "type": { "bool": true }
                },
                "showMinMax": {
                    "type": { "bool": true }
                }
            }
        },
        "colorSettings": {
            "properties": {
                "colorByCategory": {
                    "type": { "bool": true }
                },
                "conditionalColoring": {
                    "type": { "bool": true }
                },
                "aboveTargetColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "belowTargetColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "selectedColor": {
                    "type": { "fill": { "solid": { "color": true } } }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        { "for": { "in": "category" } },
                        { "for": { "in": "value" } },
                        { "for": { "in": "target" } },
                        { "for": { "in": "target2" } },
                        { "for": { "in": "minValue" } },
                        { "for": { "in": "maxValue" } },
                        { "for": { "in": "range1Max" } },
                        { "for": { "in": "range2Max" } },
                        { "for": { "in": "tooltipFields" } }
                    ]
                },
                "dataReductionAlgorithm": {
                    "top": { "count": 10000 }
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": { "default": true, "canvas": true },
        "roles": ["tooltipFields"]
    },
    "privileges": []
}
